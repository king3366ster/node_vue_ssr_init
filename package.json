{
  "name": "node-website",
  "version": "1.0.0",
  "description": "nim node website",
  "main": "index.js",
  "scripts": {
    "dev": "run-p -lns builddev:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server",
    "build": "rimraf dist && npm run build:css && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:css": "cross-env NODE_ENV=production postcss ./public/postcss/*.css -c build/postcss.config.js -d ./public/css",
    "builddev:js": "cross-env NODE_ENV=development node server",
    "builddev:css": "cross-env NODE_ENV=development postcss ./public/postcss/*.css -c build/postcss.config.js -d ./public/css --poll --watch"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@g.hz.netease.com:22222/yunxin/node-website.git"
  },
  "keywords": [
    "vue",
    "ssr",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "lru-cache": "^4.1.1",
    "mysql": "^2.14.0",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.5.3",
    "postcss-clean": "^1.0.3",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "rimraf": "^2.6.1",
    "run-all": "^1.0.1",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
